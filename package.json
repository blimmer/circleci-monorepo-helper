{
  "name": "circleci-monorepo-helper",
  "description": "A CLI tool used to generate CircleCI continuation workflows",
  "version": "0.0.0",
  "author": "Ben Limmer @blimmer",
  "bin": {
    "circleci-monorepo-helper": "./bin/run"
  },
  "bugs": "https://github.com/blimmer/circleci-monorepo-helper/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "lookpath": "^1.2.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "globby": "^10.0.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/blimmer/circleci-monorepo-helper",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  },
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "circleci-monorepo-helper",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "blimmer/circleci-monorepo-helper",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepare": "husky install",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
